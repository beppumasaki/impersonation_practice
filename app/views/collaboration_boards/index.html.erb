<% set_meta_tags title: t('.title') %>
<div class="container-fluid mt-5">
  <h1 class="text-center text-muted"><i class="fa-solid fa-people-carry-box mr-2"></i><%= t('.title') %></h1>
  <div class="collaboration-boards-wrapper text-muted">
    <div class="top-collaboration-boards pt-5 pb-4">

      <!-- カード一覧 -->
        <div class="row">
          <% if @collaboration_boards.present? %>
            <%= render partial: 'collaboration_board', collection: @collaboration_boards %>
          <% else %>
            <p>公開されているコラボはありません。</p>
          <% end %>
        </div>
    </div>
  </div>
</div>
