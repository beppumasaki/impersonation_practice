<div class="cards-wrapper text-muted">
  <div class="top-cards pt-5 pb-4">
      <%= link_to "お題を選挙に出す", new_vote_path, class:"btn btn-primary float-right mr-3" %>
  <h3 class="text-center mt-3 mb-4">お題総選挙</h3>
    <!-- カード一覧 -->
      <div class="row">
        <% if @votes.present? %>
          <%= render partial: 'vote', collection: @votes %>
        <% else %>
          <p>選挙に出馬中のお題はありません。</p>
        <% end %>
      </div>
  </div>
 </div>