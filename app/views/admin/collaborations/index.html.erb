<div class="container mb-5 pt-2">
  <h1>コラボ一覧</h1>
  <div class="row">
    <div class="col-md-12">
      <table class="table table-striped">
        <thead>
        <tr>
          <th scope="col"><%= User.human_attribute_name(:id) %></th>
          <th scope="col"><%= User.human_attribute_name(:name) %></th>
          <th scope="col"><%= Target.human_attribute_name(:created_at) %></th>
          <th scope="col"><%= Target.human_attribute_name(:name) %></th>
          <th scope="col"><%= Result.human_attribute_name(:name) %></th>
          <th scope="col"><%= Result.human_attribute_name(:collaboration_voice) %></th>
        </tr>
        </thead>
        <tbody>
          <%= render @collaborations %>
        </tbody>
      </table>
    </div>
  </div>
</div>